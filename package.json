{
  "name": "@mgasic/dynui-fixed",
  "version": "0.1.0",
  "private": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgasic/dynui-fixed.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --watch --format esm,cjs --dts",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --coverage",
    "test:a11y": "vitest run -t a11y",
    "test:contracts": "tsd",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build",
    "prepublishOnly": "pnpm build && pnpm test && pnpm typecheck && pnpm lint && pnpm test:contracts"
  },
  "files": [
    "dist",
    "src"
  ],
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@axe-core/react": "^4.7.3",
    "@storybook/addon-a11y": "^8.2.0",
    "@storybook/addon-essentials": "^8.2.0",
    "@storybook/react-vite": "^8.2.0",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.11.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.36.1",
    "jsdom": "^25.0.0",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.6.3",
    "storybook": "^8.2.0",
    "vitest": "^2.1.3",
    "chromatic": "^11.10.5",
    "tsd": "^0.30.7"
  },
  "packageManager": "pnpm@9.12.0"
}