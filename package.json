{
  "name": "@dynui/fixed-monorepo",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "test": "turbo test",
    "test:a11y": "turbo test:a11y", 
    "typecheck": "turbo typecheck",
    "lint": "turbo lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "turbo clean",
    "storybook": "cd apps/storybook && pnpm storybook",
    "build:storybook": "cd apps/storybook && pnpm build:storybook"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "vitest": "catalog:",
    "storybook": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "turbo": "^2.1.3",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10"
  },
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgasic/dynui-fixed.git"
  }
}