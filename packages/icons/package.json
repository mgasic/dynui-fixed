{
  "name": "@dynui/icons",
  "version": "0.1.0",
  "description": "Icon primitives for DynUI components.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "files": ["src", "README.md"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "eslint --config ../../eslint.config.js \"src/**/*.{ts,tsx}\" || echo \"No files to lint\"",
    "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\""
  },
  "devDependencies": {
    "@dynui/build-config": "workspace:*",
    "@dynui/eslint-config": "workspace:*",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
