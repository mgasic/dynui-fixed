{
  "name": "@dynui/core",
  "version": "1.0.0",
  "description": "DynUI Component Library - Production Ready",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": "./dist/styles.css"
  },
  "files": ["dist", "README.md"],
  "sideEffects": ["**/*.css"],
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "test": "vitest run --coverage --config vitest.config.ts",
    "test:a11y": "vitest run -t a11y --config vitest.config.ts",
    "test:contracts": "tsd",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "eslint --config ../../eslint.config.js \"src/**/*.{ts,tsx}\"",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@dynui/design-tokens": "workspace:*",
    "@dynui/icons": "workspace:*",
    "classnames": "^2.5.1"
  },
  "devDependencies": {
    "@axe-core/react": "catalog:",
    "@dynui/build-config": "workspace:*",
    "@dynui/eslint-config": "workspace:*",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@testing-library/user-event": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "eslint": "catalog:",
    "jsdom": "catalog:",
    "prettier": "catalog:",
    "tsup": "catalog:",
    "tsd": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "vitest-axe": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mgasic/dynui-fixed.git",
    "directory": "packages/core"
  },
  "keywords": [
    "react",
    "component-library",
    "typescript",
    "accessibility",
    "design-system"
  ]
}
