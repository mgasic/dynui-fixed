{
  "name": "@dynui/build-config",
  "version": "0.1.0",
  "description": "Shared build configuration primitives for DynUI packages.",
  "type": "module",
  "private": true,
  "files": [
    "src",
    "README.md",
    "tsconfig.base.json",
    "tsup.library.config.ts"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./tsconfig.base": "./tsconfig.base.json",
    "./tsconfig.base.json": "./tsconfig.base.json",
    "./tsup.library.config": "./tsup.library.config.ts"
  },
  "scripts": {
    "build": "echo 'Configuration packages do not require build output'",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "devDependencies": {
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
